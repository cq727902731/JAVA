class Computer{


	public void dvdPlay(DVD dvd){
		dvd.play();
	}
	public void dvdBurn(DVD dvd){
		dvd.burn();
	}

	public static void main(String[] args){    
	    Computer c = new Computer();
		DVD d = new LG();
		c.dvdplay(d);
		c.dvdburn(d);
		System.out.println("___________"); 
		d = new Samsung();
		c.dvdplay(d);
		c.dvdburn(d);
	}
}
		
